public static void plusMinus(List<Integer> arr) {
    double pos=0;
    double neg=0;
    double zero=0;
    int len=arr.size();
    for(int i=0;i<len;i++){
        if(arr.get(i)>0){
            pos++;
        }
        else if(arr.get(i)<0){
            neg++;
        }
        else{
            zero++;
        }
    }
    System.out.println(pos/len);
    System.out.println(neg/len);
    System.out.println(zero/len);

    }

}
